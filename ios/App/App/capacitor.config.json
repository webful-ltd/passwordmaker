{
	"appId": "uk.webful.passwordmaker",
	"appName": "Webful PasswordMaker",
	"webDir": "www/browser",
	"cordova": {
		"preferences": {
			"permissions": "none",
			"ScrollEnabled": "false",
			"BackupWebStorage": "none",
			"MixedContentMode": "1"
		}
	},
	"android": {
		"adjustMarginsForEdgeToEdge": "force"
	},
	"ios": {
		"scheme": "Webful PasswordMaker"
	},
	"plugins": {
		"PrivacyScreen": {
			"enable": true,
			"imageName": "Splashscreen"
		},
		"StatusBar": {
			"overlaysWebView": false
		}
	},
	"server": {
		"androidScheme": "http",
		"iosScheme": "ionic"
	},
	"packageClassList": [
		"PrivacyScreenPlugin",
		"CapacitorSQLitePlugin",
		"AppPlugin",
		"CAPBrowserPlugin",
		"ClipboardPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"StatusBarPlugin",
		"CDVPlugin"
	]
}
