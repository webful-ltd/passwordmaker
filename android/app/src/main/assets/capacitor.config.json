{
	"appId": "uk.webful.passwordmaker",
	"appName": "Webful PasswordMaker",
	"webDir": "www/browser",
	"cordova": {
		"preferences": {
			"permissions": "none",
			"ScrollEnabled": "false",
			"BackupWebStorage": "none",
			"MixedContentMode": "1"
		}
	},
	"ios": {
		"scheme": "Webful PasswordMaker"
	},
	"plugins": {
		"PrivacyScreen": {
			"enable": true,
			"imageName": "Splashscreen"
		},
		"StatusBar": {
			"overlaysWebView": false
		}
	},
	"server": {
		"androidScheme": "http",
		"iosScheme": "ionic"
	}
}
